rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    /**
     * TESTING MODE
     * This ruleset allows any authenticated user to read and write to any document.
     * This is intended for the prototype/testing phase to ensure smooth module integration.
     */
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}